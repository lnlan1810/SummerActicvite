<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainfragment"
    app:startDestination="@id/main1Fragment">
    <fragment
        android:id="@+id/main1Fragment"
        android:name="com.example.pets.main1Fragment"
        android:label="fragment_main1"
        tools:layout="@layout/fragment_main1" >
        <action
            android:id="@+id/action_main1Fragment_to_animalFragment2"
            app:destination="@id/animalFragment" />
        <action
            android:id="@+id/action_main1Fragment_to_animalFragment"
            app:destination="@id/animalFragment" />
    </fragment>
    <fragment
        android:id="@+id/animalFragment"
        android:name="com.example.pets.AnimalFragment"
        android:label="fragment_animal"
        tools:layout="@layout/fragment_animal" />
</navigation>